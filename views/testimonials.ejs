<!DOCTYPE html>
<html>

<head>
    <title>
        <%= title %>
    </title>
    <%- include('imports'); -%>
</head>

<body>
    <%- include('navbar'); -%>

        <div class="container">
            <% testimonials.forEach((item) => { %>
                <div>
                    <h2 class="testimonial-description">"<%= item.testimony %> "</h2>
                    <p class="testimonial-info">"<%= item.nome %>("<%= item.title %>") - <%= item.company %>"</p>
                 
                </div>
                <% }); %>
        </div>

</body>

</html>